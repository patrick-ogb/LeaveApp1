    {
        <thead style="text-align: center;">
            <tr>
                <th>No</th>
                <th>Name</th>
                <th>Start Date</th>
                <th>End Date</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody style="text-align: center; line-height: 45px;">

            <tr>
                <td>
                    @Model.EmployeeList.Name
