                    @Model.EmployeeList.EndDate
                </td>
            </tr>

        </tbody>
        <form asp-controller="Report" asp-action="IndexXX" method="post">
            <!-- Your form fields go here -->
            <div class="col-sm-9">
                <input asp-for="Name" class="form-control" />
            </div>
            <div class="col-sm-9">
                <input asp-for="Age" class="form-control" />
            </div>
            <input type="submit" value="Submit" />
